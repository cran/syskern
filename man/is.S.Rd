\name{is.S}
\alias{is.S}
\alias{is.R}
\alias{is.R.pre0.60}
\alias{is.R.pre0.63.2}
\alias{is.Splus}
\alias{is.Svanilla}
\alias{is.Splus.pre3.3}

\title{S/R Version Information}
\description{Determine the S/R version.}

\usage{
	is.S()
	is.R()
	is.R.pre0.60()
	is.R.pre0.63.2()
	is.Splus()
	is.Svanilla()
	is.Splus.pre3.3()
}

\arguments{None}
\value{TRUE or FALSE}
\details{
	If the S/R version is the one in question then TRUE is
	returned, otherwise FALSE. 
}
\seealso{
	\code{\link{is.R}}
	\code{\link{is.unix}}
}
\examples{is.R() }
\keyword{syskern}
\keyword{OS}
\keyword{S/R}

